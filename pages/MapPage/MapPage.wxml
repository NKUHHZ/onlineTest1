<map id="map" longitude="{{longitude}}" latitude="{{latitude}}" scale="14" controls="{{controls}}" bindcontroltap="controltap" markers="{{markers}}" circles="{{circles}}" bindmarkertap="markertap" polyline="{{polyline}}" bindregionchange="regionchange" show-location style="width: 100%; height: 450px;" bindtap='chooseLocation'></map>

<view class='submitView'>
  <text style='color:gray'>点击按钮上传图片</text>
  <button class='button1'bindtap='toSubmit'>点击上传</button>
</view>